= render template: 'layouts/application'
.banner
  = yield :banner_bg
  .container
    .table-image
      =image_tag 'bk-527.jpg'
    .table-container
      = yield :header
      %table.table-minimal
        %thead
          = yield :th
        %tbody
          = yield :tr_items
          = yield :tr_footers
      = yield :buttons
= render template: 'layouts/footer'
